{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.9",
  "private": true,
  "scripts": {
    "start": "npx expo start --clear",
    "dev": "npx expo start --dev-client --clear",
    "build:web": "npx expo export --platform web --output-dir dist && cp public/_redirects dist/_redirects && mkdir -p dist/.well-known && cp public/.well-known/assetlinks.json dist/.well-known/ && cp public/.well-known/apple-app-site-association dist/.well-known/ && cp public/.well-known/apple-app-site-association dist/",
    "lint": "npx expo lint",
    "build:ios:native": "rm -rf node_modules/.cache/babel-loader/* .expo ios && npx expo prebuild --platform ios --clean",
    "build:android:native": "rm -rf node_modules/.cache/babel-loader/* .expo android && npx expo prebuild --platform android --clean",
    "start:dev": "npx expo start --dev-client --clear",
    "start-clear": "npx expo start --clear",
    "android": "npx expo run:android --device",
    "android:apk": "eas build --platform android --profile preview --local --output=./SmartBites-preview.apk && adb install -r ./SmartBites-preview.apk",
    "ios:native": "rimraf -rf node_modules/.cache/babel-loader/* && rimraf -rf .expo && npx expo run:ios --device 'iPhone 13 Pro Max'",
    "ios:sim": "npx expo run:ios --device 'iPhone 13 Pro Max'",
    "ios": "npx expo run:ios --device",
    "ios:prod": "npx expo run:ios --configuration Release",
    "web": "rimraf -rf node_modules/.cache/babel-loader/* && rimraf -rf .expo && npx expo start -w -c"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo-google-fonts/lato": "^0.4.0",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.54.0",
    "axios": "^1.6.0",
    "expo": "53.0.22",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.4",
    "expo-camera": "~16.1.10",
    "expo-constants": "~17.1.3",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.3",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.7",
    "expo-navigation-bar": "~4.2.8",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.475.0",
    "openai": "^4.28.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-blob-util": "^0.22.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "rimraf": "^6.0.1",
    "@sentry/react-native": "~6.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
