{
  "expo": {
    "name": "SmartBites",
    "slug": "allergy-aware-recipe-finder",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "smartbites",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f9f8f2"
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBar": {
      "backgroundColor": "#f9f8f2",
      "barStyle": "dark-content",
      "translucent": true
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#f9f8f2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.allergy-aware-recipe-finder",
      "buildNumber": "35",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to scan items for allergy-friendly recipes.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "cooking.safeplate.allergyawarerecipefinder",
      "versionCode": 16,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android.png",
        "backgroundColor": "#f9f8f2"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-navigation-bar",
        {
          "barStyle": "dark-content",
          "backgroundColor": "#f9f8f2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a54493c7-fc4d-4aea-a8e9-7773401420b1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a54493c7-fc4d-4aea-a8e9-7773401420b1"
    }
  }
}
